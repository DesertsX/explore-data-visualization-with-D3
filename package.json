{
  "name": "explore-data-visualization-with-d3",
  "version": "0.0.0",
  "description": "Explore data visualization using D3",
  "engines": {
    "node": ">=10.3.0",
    "npm": ">=6.4.1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npx live-server --port=8000",
    "angular:start": "npm run docker:up",
    "angular:build": "npm run docker:build",
    "docker:up": "docker-compose -f docker-compose.using-d3-with-angular.yml up",
    "docker:build": "docker-compose -f docker-compose.using-d3-with-angular.yml up --remove-orphans --build --force-recreate",
    "docker:down": "docker-compose -f docker-compose.using-d3-with-angular.yml down",
    "docker:nuke": "npm run docker:nuke:containers && docker rmi -f $(docker images -q)",
    "docker:nuke:containers": "docker rm $(docker ps -aq)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheRobBrennan/explore-data-visualization-with-D3.git"
  },
  "author": "Rob Brennan <rob@therobbrennan.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TheRobBrennan/explore-data-visualization-with-D3/issues"
  },
  "homepage": "https://github.com/TheRobBrennan/explore-data-visualization-with-D3#readme",
  "devDependencies": {
    "tar": "^4.4.8"
  }
}
